{
  "name": "@hippy/hippy-vue-next-style-parser",
  "version": "unspecified",
  "description": "A css subset parser for hippy-vue-next",
  "author": "OpenHippy Team",
  "homepage": "https://hippyjs.org",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "keywords": [
    "hippy",
    "style",
    "parser"
  ],
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.esm.js",
    "dist/index.d.ts",
    "README.md"
  ],
  "repository": "https://github.com/Tencent/Hippy",
  "bugs": {
    "url": "https://github.com/Tencent/Hippy/issues"
  },
  "peerDependencies": {
    "@vue/shared": "^3.2.46"
  },
  "devDependencies": {
    "@vue/shared": "^3.2.46"
  },
  "entry": "./src/index.ts",
  "jest": {
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "@hippy-vue-next-style-parser/(.*)$": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "preset": "ts-jest",
    "setupFiles": [
      "<rootDir>/__test__/setup.ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "verbose": true
  }
}
