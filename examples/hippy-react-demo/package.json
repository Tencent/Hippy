    "@babel/preset-react": "^7.9.4",
    "@hippy/debug-server": "^2.1.2",
    "@hippy/hippy-dynamic-import-plugin": "^2.0.0",
    "babel-loader": "^8.1.0",
    "bezier-easing": "^2.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "css-loader": "^3.5.2",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.1.4",
    "unicode-loader": "^1.0.7",
    "url-loader": "^4.0.0",
    "webpack": "^4.43.0",
