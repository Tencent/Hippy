{
  "name": "hippy-react-demo",
  "version": "2.0.0",
  "private": true,
  "main": "src/main.js",
  "author": "XQ Kuang <x@kxq.io>",
  "license": "Apache-2.0",
  "description": "Demo app for hippy-react",
  "repository": "https://github.com/Tencent/Hippy/tree/master/examples/hippy-react-demo",
  "scripts": {
    "serve": "webpack-dev-server --config ./scripts/hippy-webpack.web.js",
    "build": "webpack --config ./scripts/hippy-webpack.web.js",
    "hippy:debug": "hippy-debug",
    "hippy:dev": "webpack --config ./scripts/hippy-webpack.dev.js",
    "hippy:vendor": "webpack --config ./scripts/hippy-webpack.ios-vendor.js --config ./scripts/hippy-webpack.android-vendor.js",
    "hippy:build": "webpack --config ./scripts/hippy-webpack.ios.js --config ./scripts/hippy-webpack.android.js"
