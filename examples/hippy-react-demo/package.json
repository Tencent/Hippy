  "description": "Demo app for hippy-react",
  "repository": "https://github.com/Tencent/Hippy/tree/master/examples/hippy-react-demo",
  "scripts": {
    "serve": "webpack-dev-server --mode=development --config ./scripts/hippy-webpack.web.js",
